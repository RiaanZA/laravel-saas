{
    "name": "riaan-za/laravel-subscription-management",
    "description": "A comprehensive Laravel package for SaaS subscription management with Vue 3, Inertia.js, and Peach Payments integration",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "subscription",
        "saas",
        "peach-payments",
        "vue3",
        "inertia",
        "tailwind"
    ],
    "authors": [
        {
            "name": "Riaan ZA",
            "email": "loophole1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "inertiajs/inertia-laravel": "^2.0",
        "peachpayments/laravel-subscriptions": "dev-master",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.4",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RiaanZA\\LaravelSubscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiaanZA\\LaravelSubscription\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiaanZA\\LaravelSubscription\\LaravelSubscriptionServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/RiaanZA/peachpayments-laravel-subscriptions"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
